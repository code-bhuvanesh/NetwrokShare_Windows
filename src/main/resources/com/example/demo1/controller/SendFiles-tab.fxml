<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="544.0" prefWidth="760.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo1.controller.SendFilesTab">

   <StackPane fx:id="fileViewer" layoutX="5.0" layoutY="114.0" prefHeight="355.0" prefWidth="750.0" style="-fx-background-color: #e6d4cc; -fx-border-color: #615c5a; -fx-border-width: 5; -fx-border-radius: 5;">
      <Label text="drag files here">
         <font>
            <Font size="17.0" />
         </font>
      </Label>
   </StackPane>
   <Button fx:id="clearAllBtn" layoutX="305.0" layoutY="506.0" mnemonicParsing="false" text="Clear all" />
   <Button fx:id="sendFilesBtn" layoutX="374.0" layoutY="506.0" mnemonicParsing="false" text="send Files" />
   <Button fx:id="selectFilesBtn" layoutX="643.0" layoutY="506.0" mnemonicParsing="false" onAction="#selectFiles" text="select files" />
</AnchorPane>
